                    </div>
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Name</th>                       
                                <th>Email</th>
                                <th>Mobile Number</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% uData.forEach((uData, index) => { %>
                            <tr>
                                <td><%=num++ %> </td>
                                <!-- <td><a href="#"><img src="/examples/images/avatar/1.jpg" class="avatar" alt="Avatar"> <%= uData.username %></a></td> -->
                                <td><%= uData.username %></td>                        
                                <td><%= uData.email %></td>
                                <td><%= uData.mobile %></td>
                                <td><span class="status text-success">&bull;</span> Active</td>
                                <td>
                                   <form  method="post" action="admin/block">
                                   <% if(uData.block){ %>  
                                   <button type="submit" name="unblock_btn" value="ublock"  class="btn btn-danger btn-xs">Unblock</button>
                                   
                                  <%}else{  %>  
                                    <button type="submit"  name="block_btn" value="block" class="btn btn-danger btn-xs">Block</button>
                                   <% } %> 
                                  </form>
                                </td>
                            </tr>
                            <% }) %>
                        </tbody>
                    </table>
