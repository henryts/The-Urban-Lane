                                                        <button type="submit" class="btn btn-fill-out submit">Save</button>
                                                      </div>
                                                    </div>
                                                  </form>
                                                  
                                                  <script>
 $(document).ready(function(){
  $('#passwordResetForm').submit(function(e){
    e.preventDefault();
    var formData = $(this).serialize();
    $.ajax({
      type: 'POST',
      url: '/passwordReset',
      data: formData,
      success: function(response){
        console.log(response);
        // Password updated successfully, redirect to homepage
        window.location.href = '/';
      },
      error: function(xhr, status, error){
       console.log("erorrrr",xhr.responseText);

        // Display the error message in the form
        $('#passprob').text(xhr.responseText).show();
        $('#passprob').html('<div class="alert alert-danger">'+xhr.responseText+'</div>').show();

       console.log("erorrrr",response.message);
        $('#passprob').append('<div class="alert alert-danger">'+xhr.responseText+'</div>');
      }
    });
  });
});

