<%- include('../partials/userHeader') %> 
    <section class="mt-50 mb-50">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="table-responsive">
                        <table class="table shopping-summery text-center">
                            <thead>
                                <tr class="main-heading">
                                    <th scope="col" colspan="2">Product</th>
                                    <th scope="col">Price</th>
                                    <th scope="col">Stock Status</th>
                                    <th scope="col">Action</th>
                                    <th scope="col">Remove</th>
                                </tr>
                            </thead>

                            <tbody>
                             
                                <tr>
                                    <% for(let i=0;i<wList.length;i++)  { %> 
                                     <td class="image product-thumbnail"><img src="/uploads/<%=wList[i].productImages[1].filename%>" alt="#"></td> 
                                    <td class="product-des product-name">
                                        <h5 class="product-name"><a href="shop-product-right.html"><%=wList[i].productName%></a></h5>
                                        <p class="font-xs">Maboriosam in a tonto nesciung eget<br> distingy magndapibus.
                                        </p>
                                    </td>
                                    <td class="price" data-title="Price"><span><%=wList[i].productCost%></span></td>
                                    <td class="text-center" data-title="Stock">
                                        <span class="color3 font-weight-bold">In Stock</span>
                                    </td>
                                    <td class="text-right" data-title="Cart">
                                        <button class="btn btn-sm"><i class="fi-rs-shopping-bag mr-5"></i>Add to cart</button>
                                    </td>
                                    <td class="action" data-title="Remove"><a href="/removefromWishlist/<%=wList[i].productsId%>"><i class="fi-rs-trash"></i></a></td>
                                </tr>
                                 <% } %> 
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>

